{
	"version": "0.2.0",
	"configurations": [
		{
			"cwd": "${workspaceRoot}",
			"executable": "${workspaceRoot}/_TARGET_",
			"name": "Launch with OpenOCD",
			"request": "launch",
			"type": "cortex-debug",
			"servertype": "openocd",
			"runToMain": true,
			"interface": "swd",
			"device": "STM32F405rgtx",
			"svdFile": ".vscode/STM32F405.svd",
			"configFiles": [
				"interface/stlink.cfg",
				"target/stm32f4x.cfg"
			],
			"preLaunchTask": "Make Debug",
			"swoConfig": {
				"enabled": true,
				"source": "probe",
				"swoFrequency": 100000,
				"cpuFrequency": 168000000,
				"decoders": [
					{
						"port": 0,
						"type": "console",
						"label": "SWO output",
						"encoding": "ascii"
					},
					{
						"type": "graph",
						"port": 1,
						"encoding": "float",
						"graphId": "sysload",
						"scale": 1
					}
				]
			},
			// Uncomment this to show real time system load graph
			// "graphConfig": [
			// 	{
			// 		"label": "System Load",
			// 		"timespan": 25,
			// 		"type": "realtime",
			// 		"annotate": false,
			// 		"maximum": 100,
			// 		"minimum": 0,
			// 		"plots": [
			// 			{
			// 				"graphId": "sysload",
			// 				"label": "System Load",
			// 				"color": "#00acab"
			// 			}
			// 		]
			// 	},
			// ]
		}
	]
}